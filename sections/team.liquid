        <div class="padding-global">
          <div class="padding-section-3-75">
            <div class="container-large">
              <div class="tabs_wrapper is-team">
                <h2>
{{ section.settings.text_heading }}
</h2>
                <div x-data="{tab: 1}" class="tabs_element is-etapes">
                  <div li-for:inside="block in section.blocks" class="tabs_menu is-team">
        {% for block in section.blocks %}                            {% if block.type == 'tab' %}    <button x-bind:class="tab === {{forloop.index}} ? 'is-active' : ''" x-on:click="tab = {{forloop.index}}" role="tab" aria-controls="panel-{{section.index}}-{{forloop.index}}" tabindex="0" id="tab-{{section.index}}-{{forloop.index}}" x-bind:aria-selected="tab === {{forloop.index}} ? 'true' : 'false'" li-if="block.type == 'tab'" class="button is-team-tab">
    
                      <div li-object="block.settings.text_tab_label">{{ block.settings.text_tab_label }}</div>
                        </button>
{% endif %}
                    {% endfor %}    </div>
                  <div class="tabs_body">
                    {% for block in section.blocks %}
    {% if block.type == "tab" %}
                    <div id="panel-{{section.index}}-{{forloop.index}}" x-show="tab === {{forloop.index}}" aria-labelledby="tab-{{section.index}}-{{forloop.index}}" role="tabpanel" x-bind:aria-hidden="tab === {{forloop.index}} ? 'false' : 'true'" x-cloaks="" li-block="Tab" class="tabs_panel">
                                    
                      
                                    <div class="tabs_panel-content is-team">
                        <article class="tabs_panel-article">
                          <div class="tabs_panel-article-heading">
{{ block.settings.text_titre }}
</div>
                          <div class="rich-text-tabs w-richtext"> {{ block.settings.richtext_contenu }} </div>
                        </article>
                        <div class="tabs_panel-image-wrapper">{% if block.settings.image_image %}
{{ block.settings.image_image | image_url: width: 1000 | image_tag: loading: 'lazy', class: 'tabs_panel-image' }}
{% else %}
<img loading="lazy" alt="" src="{{ 'partenaires.avif' | asset_url }}" class="tabs_panel-image">
{% endif %}{% if block.settings.image_image_2 %}
{{ block.settings.image_image_2 | image_url: width: 1000 | image_tag: loading: 'lazy', class: 'tabs_panel-image' }}
{% else %}
<img loading="lazy" alt="" src="{{ 'reseaux-sociaux.avif' | asset_url }}" class="tabs_panel-image">
{% endif %}</div>
                      </div>
                                    
                      
                                    <div class="hide">
{{ block.settings.text_tab_label }}
</div>
                                    
                    
                            </div>
            {% endif %}
{% endfor %}
                  </div>
                </div>
                <ul li-for:inside="block in section.blocks" x-data="{open: 1}" role="list" class="faq_list is-tabs">
        {% for block in section.blocks %}                            
                  
                            <li :class="open === {{forloop.index}} ? 'is-active' : ''" role="button" tabindex="0" aria-controls="faq-body-{{forloop.index}}" x-on:click="open = (open === {{forloop.index}} ? 0 : {{forloop.index}}); $el.setAttribute('aria-expanded', open === {{forloop.index}} ? 'true' : 'false')" class="tabs_item is-active">
                    <div class="faq_item-heading">
                      <div li-object="block.settings.text_tab_label" class="heading-style-h4">{{ block.settings.text_tab_label }}</div>
