<div class="padding-global padding-section-small">
          <div class="container-large">
            <div class="headline-wrapper">
              <h2 id="w-node-_7efc0c64-e28a-5341-1e13-d144ac0beb96-0526affc" class="heading-style-h2">
{{ section.settings.text_heading }}
</h2>
              <a href="{{ routes.root_url }}" li-object:href="routes.root_url" class="text-size-small text-style-link hide-mobile-landscape"><span>
{{ section.settings.text_continue_shopping }}
</span></a>
            </div>
            {% if cart.item_count > 0 %}    <div li-if="cart.item_count > 0">
    
              <div class="cart_form-block w-form">
                {% form 'cart', cart, class: 'cart_grid-wrapper' %}
<div class="cart_wrapper-left">
                    <div class="cart_table is-list">
                      <div class="cart_grid is-header w-hidden-tiny">
                        <div id="w-node-_7efc0c64-e28a-5341-1e13-d144ac0beb9f-0526affc" class="cart_table-header">
                          <p class="text-size-regular">
{{ section.settings.text_product }}
</p>
                        </div>
                        <div class="cart_table-header">
                          <p class="text-size-regular">
{{ section.settings.text_quantity }}
</p>
                        </div>
                        <div id="w-node-_7efc0c64-e28a-5341-1e13-d144ac0beba5-0526affc" class="cart_table-header">
                          <p class="text-size-regular">
{{ section.settings.text_price }}
</p>
                        </div>
                      </div>
                      {% for item in cart.items %}    <div li-for="item in cart.items" class="cart_item">
                                    
                        
                            <div id="w-node-_7efc0c64-e28a-5341-1e13-d144ac0beba9-0526affc" class="display-inlineflex">
                          <a href="{{ item.url }}" li-object:href="item.url" id="w-node-_7efc0c64-e28a-5341-1e13-d144ac0bebaa-0526affc" class="w-inline-block"><img li-object:src="item.image |  img_url: 'small'" src="{{ item.image |  img_url: 'small' }}" alt="" class="cart_image"></a>
                          <div class="cart_info-order">
                            <a href="{{ item.url }}" li-object:href="item.url" li-object="item.title" class="text-size-regular">{{ item.title }}</a>
                            <div class="hide">
                              <p item="price" class="cart_price">$13</p>
                              <p item="original-price" class="cart_price away">$18</p>
                            </div>
                          </div>
                        </div>
                            
                        
                            <div id="w-node-_7efc0c64-e28a-5341-1e13-d144ac0bebb4-0526affc" class="cart_table-item anzahl">
                          <div class="cart_quantity-wrapper">
                            <div>Quantité</div><input class="cart_quantity w-input" li-object:value="item.quantity" maxlength="256" name="updates[]-2" data-name="Updates 2" placeholder="1" type="number" id="updates[]-2" required="" value="{{ item.quantity }}">
                          </div>
                        </div>
                            
                        
                            <div id="w-node-_7efc0c64-e28a-5341-1e13-d144ac0bebbc-0526affc" class="cart_price-info">
                          <div li-object="item.final_line_price | money" class="text-style-tagline">{{ item.final_line_price | money }}</div>
                          {% if item.original_line_price > item.final_line_price %}    <div li-if="item.original_line_price > item.final_line_price">
    
                            <div li-object="item.original_line_price | money" class="text-size-regular text-style-strikethrough">{{ item.original_line_price | money }}</div>
                              </div>
{% endif %}
                        </div>
                            
                        
                            <a href="{{ item.url_to_remove }}" li-object:href="item.url_to_remove" class="cart_delete-element w-inline-block">
                          <div li-element="mini-cart-item-remove" class="icon-tash-cart w-embed" :productprop="product" @click="$dispatch('setcartitem', { product: product.key, quantity: 0 })"><svg width="18" height="20" viewbox="0 0 18 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                              <path d="M15.2087 4.01367L14.675 12.6464C14.5387 14.852 14.4705 15.9548 13.9176 16.7477C13.6443 17.1397 13.2924 17.4705 12.8843 17.7191C12.0588 18.222 10.9539 18.222 8.74408 18.222C6.53134 18.222 5.42498 18.222 4.59895 17.7182C4.19057 17.4691 3.83854 17.1377 3.5653 16.7451C3.01263 15.9509 2.94597 14.8465 2.81263 12.6378L2.29199 4.01367" stroke="#545454" stroke-width="1.66667" stroke-linecap="round"></path>
                              <path d="M1 4.01389H16.5M12.2424 4.01389L11.6546 2.80121C11.2641 1.99567 11.0688 1.59289 10.732 1.34169C10.6573 1.28597 10.5782 1.23641 10.4955 1.19349C10.1225 1 9.67493 1 8.77973 1C7.86205 1 7.40321 1 7.02406 1.2016C6.94003 1.24628 6.85985 1.29785 6.78434 1.35578C6.44363 1.61716 6.25332 2.03467 5.87269 2.86969L5.35113 4.01389" stroke="#545454" stroke-width="1.66667" stroke-linecap="round"></path>
                              <path d="M6.59766 13.4863L6.59766 8.31966" stroke="#545454" stroke-width="1.66667" stroke-linecap="round"></path>
                              <path d="M10.9023 13.487L10.9023 8.32031" stroke="#545454" stroke-width="1.66667" stroke-linecap="round"></path>
                            </svg></div>
                        </a>
                            
                      
                        </div>
{% endfor %}
                    </div>
                  </div>
                  <div class="cart_wrapper-right">
                    <div id="w-node-_7efc0c64-e28a-5341-1e13-d144ac0bebc3-0526affc" class="cart_table outline-solid-1">
                      <div class="cart_summary-header w-hidden-tiny">
                        <div id="w-node-_7efc0c64-e28a-5341-1e13-d144ac0bebc5-0526affc" class="cart_table-header">
                          <p class="text-size-regular">
{{ section.settings.text_summary }}
</p>
                        </div>
                      </div>
                      <div class="cart_content">
                        <div class="cart_wrapper">
                          <div class="cart_details-wrapper">
                            <div class="text-size-regular">
{{ section.settings.text_sum_inkl_vat }}
</div>
                            <div class="cart_details-price">
                              {% if cart.total_discounts > 0 %}    <div li-if="cart.total_discounts > 0">
    
                                <div li-object="cart.original_total_price | money" class="text-style-strikethrough">{{ cart.original_total_price | money }}</div>
                                  </div>
{% endif %}
                              <div li-object="cart.total_price | money" class="text-style-tagline">{{ cart.total_price | money }}</div>
                            </div>
                          </div>
                          {% if cart.total_discounts > 0 %}    <div li-if="cart.total_discounts > 0" class="cart_details-wrapper">
    
                            <div class="text-size-regular">
{{ section.settings.text_discount }}
</div>
                            <div li-object="cart.total_discount | money">{{ cart.total_discount | money }}</div>
                              </div>
{% endif %}
                          <div class="cart_details-wrapper">
                            <div class="text-size-small">Vous gagner <span class="fidelity_count text-style-underline">20</span><span class="text-color-orange text-style-underline"> points de fidélité</span> pour cette commande</div>
                          </div>
                        </div>
                        <div class="cart_action"><input type="submit" data-wait="Please wait..." class="button is-secondary is-cart w-button" value="Update cart"></div>
                      </div>
                    </div>
                    <div class="cart_table outline-solid-1">
                      <div class="cart_note-header w-hidden-tiny">
                        <div id="w-node-_7efc0c64-e28a-5341-1e13-d144ac0bebdd-0526affc" class="cart_table-header">
                          <p class="text-size-regular">
{{ section.settings.text_comment }}
</p>
                        </div>
                      </div>
                      <div class="cart_content">
                        <div class="form_field-wrapper"><textarea class="form_input is-text-area w-input" maxlength="5000" name="note" data-name="field" placeholder="Vous pouvez laisser un commentaire" id="CartNote"></textarea></div>
                      </div>
                    </div>
                  </div>
                {% endform %}
                <div class="w-form-done">
                  <div>Thank you! Your submission has been received!</div>
                </div>
                <div class="w-form-fail">
                  <div>Oops! Something went wrong while submitting the form.</div>
                </div>
              </div>
              <div li-content-for-blocks="">
{% for block in section.blocks %}
{% case block.type %}
{% when '@app' %}
{% render block %}

                {% when 'bloc-vide' %}
            <div li-block="Bloc vide">
                    
                  
                    <div>
{{ block.settings.text_texte }}
</div>
                    
                
                </div>
                {% when 'bloc-vide-2' %}
            <div li-block="Bloc vide 2">
                    
                  
                    <div>
{{ block.settings.text_texte_2 }}
</div>
                    
                
                </div>
              {% endcase %}
{% endfor %}

    </div>
                </div>
{% endif %}
            {% if cart.item_count == 0 %}    <div li-if="cart.item_count == 0" class="cart_empty outline-solid-1">
    
              <div class="empty_cart-heading">
                <div class="heading-style-h5">
{{ section.settings.text_cart_empty }}
</div>
                <div>Il semble que rien ne ce trouve dans ce panier pour le moment</div>
              </div><img src="{{ 'panier-empty.svg' | asset_url }}" loading="lazy" alt="">
              <a href="{{ routes.root_url }}" li-object:href="routes.root_url" class="button is-cart w-button">Découvrir tous nos produits</a>
                </div>
{% endif %}
          </div>
